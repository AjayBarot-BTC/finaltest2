<%= form_for @projectdetail, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :proj_name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :proj_name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :proj_code, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :proj_code, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :proj_desc, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :proj_desc, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :biling_type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :biling_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :start_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :start_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :dead_line_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :dead_line_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :end_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :end_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :github_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :github_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :status, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :status, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                projectdetails_path, :class => 'btn' %>
  </div>
<% end %>
