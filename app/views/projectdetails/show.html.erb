<%- model_class = Projectdetail -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:proj_name) %>:</strong></dt>
  <dd><%= @projectdetail.proj_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:proj_code) %>:</strong></dt>
  <dd><%= @projectdetail.proj_code %></dd>
  <dt><strong><%= model_class.human_attribute_name(:proj_desc) %>:</strong></dt>
  <dd><%= @projectdetail.proj_desc %></dd>
  <dt><strong><%= model_class.human_attribute_name(:biling_type) %>:</strong></dt>
  <dd><%= @projectdetail.biling_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:start_date) %>:</strong></dt>
  <dd><%= @projectdetail.start_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dead_line_date) %>:</strong></dt>
  <dd><%= @projectdetail.dead_line_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:end_date) %>:</strong></dt>
  <dd><%= @projectdetail.end_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:github_url) %>:</strong></dt>
  <dd><%= @projectdetail.github_url %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @projectdetail.status %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              projectdetails_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_projectdetail_path(@projectdetail), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              projectdetail_path(@projectdetail),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
